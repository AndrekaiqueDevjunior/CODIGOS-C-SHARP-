using System;

namespace menudeopcoes
{

    class Program
    {
        static void Main(string[] args)
        {
            string[] palavras = { "André", "Kaique", "Guiadoprogramador", "formação", "Node", "Curso de programação" };

            for (int i = 0; i < palavras.Length ; i++)
            {

                Console.Write(palavras[i]); // nao colocar o WriteLine, da a opção de agrupar e sequenciar a ordem.
                Console.WriteLine(" - Zigurate - To the TOP  Serviços Gerais de Gestão");



            }

            Console.ReadLine();


        }
    }

}
